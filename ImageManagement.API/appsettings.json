{
  "allowedExtensions": ".JPG,.PNG,.WebP",
  "MaxFileSize": "2*1024*1024",
  "AllowedSizes": "phone,tablet,desktop",
  "ConversionFormat": "WebP",
  "TargetSizes": [
    {
      "Name": "phone",
      "Width": 640,
      "Height": 480
    },
    {
      "Name": "tablet",
      "Width": 1024,
      "Height": 768
    },
    {
      "Name": "desktop",
      "Width": 1920,
      "Height": 1080
    }
  ],
  "UploadFolderName": "Uploads",
  "ExIFFileName": "exif.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
